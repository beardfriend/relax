{
  "name": "server-hire",
  "version": "1.0.0",
  "description": "yogaHire_server",
  "main": "./src/app.ts",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --files --respawn -r tsconfig-paths/register src/app.ts",
    "start": "NODE_ENV=production ts-node-dev  --files --respawn -r tsconfig-paths/register src/app.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:revert",
    "migration:create": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:create -n $NAME"
  },
  "author": "",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "es6-shim": "^0.35.6",
    "express": "^4.17.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "type-check": "^0.4.0",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^4.0.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.13",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1"
  }
}
