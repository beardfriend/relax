{
  "name": "server-hire",
  "version": "1.0.0",
  "description": "yogaHire_server",
  "main": "./src/app.ts",
  "scripts": {
    "dev": "NODE_NEV=development ts-node-dev --respawn -r tsconfig-paths/register src/app.ts",
    "start": "NODE_ENV=production ts-node-dev --respawn -r tsconfig-paths/register src/app.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:revert",
    "migration:create": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:create -n $NAME"
  },
  "author": "",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "type-check": "^0.4.0",
    "typeorm": "^0.2.41"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^4.29.3",
    "casual": "^1.6.2",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1"
  }
}
